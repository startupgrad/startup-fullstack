{
  "name": "ideanick",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "b": "pnpm --filter @ideanick/backend",
    "w": "pnpm --filter @ideanick/webapp",
    "sh": "pnpm --filter @ideanick/shared",
    "dev": "pnpm -r --parallel dev",
    "types": "concurrently 'pnpm b types' 'pnpm w types' 'pnpm sh types'",
    "lint": "concurrently 'pnpm b lint' 'pnpm w lint' 'pnpm sh lint'",
    "prettify": "pnpm -r --parallel prettify",
    "prepare": "husky install"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "rimraf": "^4.1.2",
    "ts-node-dev": "^2.0.0",
    "ts-patch": "^2.1.0",
    "typescript": "^4.9.4",
    "typescript-transform-paths": "^3.4.6"
  }
}
