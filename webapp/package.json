{
  "name": "@ideanick/webapp",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "test": "jest",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "types": "tsc --noEmit --project ./tsconfig.json && tsc --noEmit --project ./tsconfig.node.json",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache .",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache 'src/**/*.scss'",
    "stylecheck": "sass --update -q ./src:./node_modules/.cache/.stylecheckcache",
    "prettify": "prettier --loglevel warn --cache --write '{src/**/*,*}.{ts,tsx,js,json,yml,scss}' '!**/{node_modules,dist}/**/*'"
  },
  "dependencies": {
    "@ideanick/backend": "workspace:*",
    "@ideanick/shared": "workspace:*",
    "@nanostores/react": "^0.4.1",
    "@sentry/react": "^7.35.0",
    "@tanstack/react-query": "^4.20.4",
    "@trpc/client": "^10.7.0",
    "@trpc/react-query": "^10.7.0",
    "@trpc/server": "^10.7.0",
    "classnames": "^2.3.2",
    "formik": "^2.2.9",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "nanostores": "^0.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-icons": "^4.7.1",
    "react-infinite-scroller": "^1.2.6",
    "react-router-dom": "^6.6.1",
    "usehooks-ts": "^2.9.1",
    "zod": "^3.20.2",
    "zod-formik-adapter": "^1.2.0"
  },
  "devDependencies": {
    "@sentry/vite-plugin": "^0.4.0",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-infinite-scroller": "^1.2.3",
    "@vitejs/plugin-react": "^3.0.0",
    "include-media": "^1.4.10",
    "reset-css": "^5.0.1",
    "sass": "^1.57.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^6.1.0",
    "vite": "^4.0.0",
    "vite-plugin-svgr": "^2.4.0"
  }
}
